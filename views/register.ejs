<%- include('head', { title: 'my custom page' }) -%>

		<!-- BREADCRUMB -->
		<div id="breadcrumb" class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">
					<div class="col-md-12">
						<h3 class="breadcrumb-header">Registration</h3>
						<ul class="breadcrumb-tree">
							<li><a href="/">Home</a></li>
							<li class="active">Sign up</li>
						</ul>
					</div>
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /BREADCRUMB -->

		<!-- SECTION -->
		<div class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">

					<div class="col-md-7">
						<!-- Register Details -->
						<div class="billing-details">
							<div class="section-title col-md-12">
								<h3 class="title">Create Account</h3>
							</div>
							<p class="col-md-12">You Already have an account ? <a href="/login">Login here .</a></p>

							<form action="" method="post">
							<div class="form-group col-md-12">
								<input class="input" type="text" name="username" placeholder="Username*" required>
							</div>
							<div class="form-group col-md-6">
								<input class="input" type="text" name="firstname" placeholder="First Name*" required>
							</div>
							<div class="form-group col-md-6">
								<input class="input" type="text" name="lastname" placeholder="Last Name*" required>
							</div>
							<div class="form-group col-md-12">
								<input class="input" type="email" name="email" placeholder="Email*" required>
							</div>
							<div class="form-group col-md-12">
								<input class="input" type="password" name="password" placeholder="Password*" required>
							</div>
							<div class="form-group col-md-12">
								<input class="input" type="text" name="phone" placeholder="Phone">
							</div>
							<div class="form-group col-md-12">
								<input class="input" type="text" name="address" placeholder="Address">
							</div>
							<div class="form-group col-md-4">
								<input class="input" type="text" name="city" placeholder="City">
							</div>
							<div class="form-group col-md-4">
								<input class="input" type="text" name="country" placeholder="Country">
							</div>
							<div class="form-group col-md-4">
								<input class="input" type="text" name="zipcode" placeholder="ZIP Code" maxlength="5"> 
							</div>
							<div class="form-group col-md-12">
								<button type="submit" class="primary-btn btn-lg order-submit" style="width: 100%;">Sign up</button>
							</div>
							
							</form>

						</div>
						<!-- /Register Details -->
					</div>	
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /SECTION -->

<%- include('foot') -%>		
